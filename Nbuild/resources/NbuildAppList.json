{
  "NbuildAppList": [
    {
      "Name": "Ntools",
      "Version": "1.1.0",
      "AppFileName": "nb.exe",
      "WebDownloadFile": "https://github.com/naz-hage/ntools/releases/download/$(Version)/$(Version).zip",
      "DownloadedFile": "$(Version).zip",
      "InstallCommand": "powershell.exe",
      "InstallArgs": "-Command Expand-Archive -Path $(Version).zip -DestinationPath $(InstallPath) -Force",
      "InstallPath": "$(ProgramFiles)\\Nbuild"
    },
    {
      "Name": "VS MS Build Tools",
      "Version": "17.8.5.5502",
      "AppFileName": "msbuild.exe",
      "WebDownloadFile": "https://aka.ms/vs/17/release/vs_BuildTools.exe",
      "DownloadedFile": "vs_BuildTools.exe",
      "InstallCommand": "vs_BuildTools.exe",
      "InstallArgs": "--add Microsoft.VisualStudio.Workload.MSBuildTools Microsoft.NetCore.Component.SDK Microsoft.VisualStudio.Workload.ManagedDesktop Microsoft.VisualStudio.Workload.NetWeb --quiet",
      "InstallPath": "$(ProgramFilesX86)\\Microsoft Visual Studio\\2022\\BuildTools\\MSBuild\\Current\\Bin\\amd64"
    },
    {
      "Name": "Visual Studio 2022",
      "Version": "17.0.0",
      "AppFileName": "devenv.exe",
      "WebDownloadFile": "https://aka.ms/vs/17/release/vs_Community.exe",
      "DownloadedFile": "vs_Community.exe",
      "InstallCommand": "vs_Community.exe",
      "InstallArgs": "--add Microsoft.VisualStudio.Workload.NetWeb Microsoft.VisualStudio.Workload.NetDesktop Microsoft.VisualStudio.Workload.Azure Microsoft.VisualStudio.Workload.Data --quiet",
      "InstallPath": "$(ProgramFiles)\\Microsoft Visual Studio\\2022\\Community\\Common7\\IDE"
    },
    {
      "Name": "7-zip",
      "Version": "23.01",
      "AppFileName": "7z.exe",
      "WebDownloadFile": "https://www.7-zip.org/a/7z2301-x64.exe",
      "DownloadedFile": "7zip.exe",
      "InstallCommand": "7zip.exe",
      "InstallArgs": "/S /D=\"$(ProgramFiles)\\7-Zip\"",
      "InstallPath": "$(ProgramFiles)\\7-Zip"
    },
    {
      "Name": "Git for Windows",
      "Version": "2.43.0",
      "AppFileName": "git.exe",
      "WebDownloadFile": "https://github.com/git-for-windows/git/releases/download/v$(Version).windows.1/Git-$(Version)-64-bit.exe",
      "DownloadedFile": "Git-$(Version)-64-bit.exe",
      "InstallCommand": "Git-$(Version)-64-bit.exe",
      "InstallArgs": "/SILENT /NORESTART /CLOSEAPPLICATIONS /RESTARTAPPLICATIONS /SP- /LOG",
      "InstallPath": "$(ProgramFiles)\\Git\\bin"
    },
    {
      "Name": "Powershell",
      "Version": "7.4.1",
      "AppFileName": "pwsh.exe",
      "WebDownloadFile": "https://github.com/PowerShell/PowerShell/releases/download/v$(Version)/PowerShell-$(Version)-win-x64.msi",
      "DownloadedFile": "PowerShell-$(Version)-win-x64.msi",
      "InstallCommand": "msiexec.exe",
      "InstallArgs": "/i PowerShell-$(Version)-win-x64.msi /quiet",
      "InstallPath": "$(ProgramFiles)\\PowerShell\\7"
    }
  ]
}
